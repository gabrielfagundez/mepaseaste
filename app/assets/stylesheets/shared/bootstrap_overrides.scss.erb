@font-face {
  font-family: 'FontAwesome';
  src: url('<%= asset_path('font-awesome/fonts/font-awesome/fonts/fontawesome-webfont.eot') %>');
  src: url('<%= asset_path('font-awesome/fonts/fontawesome-webfont.eot') + '?#iefix' %>') format('eot'), url('<%= asset_path('font-awesome/fonts/fontawesome-webfont.woff') %>') format('woff'), url('<%= asset_path('font-awesome/fonts/fontawesome-webfont.ttf') %>') format('truetype'), url('<%= asset_path('font-awesome/fonts/fontawesome-webfont.svg') + '#FontAwesome' %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Glyphicons Halflings';
  src: url('<%= asset_path('bootstrap/dist/fonts/glyphicons-halflings-regular.eot') %>');
  src: url('<%= asset_path('bootstrap/dist/fonts/glyphicons-halflings-regular.eot') + '?#iefix' %>') format('embedded-opentype'), url('<%= asset_path('bootstrap/dist/fonts/glyphicons-halflings-regular.woff') %>') format('woff'), url('<%= asset_path('bootstrap/dist/fonts/glyphicons-halflings-regular.ttf') %>') format('truetype'), url('<%= asset_path('bootstrap/dist/fonts/glyphicons-halflings-regular.svg#glyphicons_halflingsregular') %> ') format('svg');
}

@font-face {
  font-family: 'Ionicons';
  src: url('<%= asset_path('ionicons/fonts/ionicons.eot') + '?v=1.5.2' %>');
  src: url('<%= asset_path('ionicons/fonts/ionicons.eot') + '?v=1.5.2#iefix' %>') format('embedded-opentype'), url('<%= asset_path('ionicons/fonts/ionicons.ttf') + '?v=1.5.2' %>') format('truetype'), url('<%= asset_path('ionicons/fonts/ionicons.woff') + 'v=1.5.2' %>') format('woff'), url('<%= asset_path('ionicons/fonts/ionicons.svg') + 'v=1.5.2#Ionicons' %>') format('svg'); font-weight: normal; font-style: normal;
}